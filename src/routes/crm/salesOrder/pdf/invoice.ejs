        </div>  

        <!-- SECTION After commercial list  -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-2">
                    <h4>Currency</h4>
                    <h4>Sub Total</h4>
                    <h4>GST</h4>
                    <h4>Grand Total</h4>
                    <h4>Sales PIC</h4>
                    <h4>Shipper</h4>
                </div>
                <div class="col-10">
                    <h4> <%= salesOrderPayLoad.currency %></h4>
                    <h4> <%= salesOrderPayLoad.total %></h4>
                    <h4> <%= salesOrderPayLoad.gst %></h4>
                    <h4> <%= salesOrderPayLoad.total + salesOrderPayLoad.gst %></h4>
                    <h4> <%= salesOrderPayLoad.salesPic %></h4>
