<style>
    @font-face {
        font-family: "Roboto";
        src: url("../font/roboto/Roboto-Regular.eot");
        src: url("../font/roboto/Roboto-Regular.woff") format("woff"),
            url("../font/roboto/Roboto-Regular.ttf") format("ttf");
    }

    @font-face {
        font-family: "Roboto";
        src: url("../font/roboto/Roboto-Bold.eot");
        src: url("../font/roboto/Roboto-Bold.woff") format("woff"),
            url("../font/roboto/Roboto-Bold.ttf") format("ttf");
        font-weight: bold;
    }

    @font-face {
        font-family: "Roboto";
        src: url("../font/roboto/Roboto-Light.eot");
        src: url("../font/roboto/Roboto-Light.woff") format("woff"),
            url("../font/roboto/Roboto-Light.ttf") format("ttf");
        font-weight: lighter;
    }

    body {
        background: white;
    }

    h4 {
        font-size: 14px;
        margin: 0;
    }

    p {
        font-size: 11px;
        margin: 0;
    }

    page {
        font-family: "Roboto";
        background: white;
        display: block;
        margin: 0;
        margin-bottom: 0cm;
        box-shadow: 0 0 0.5cm rgba(0, 0, 0, 0.5);
    }

    page[size="A4"] {
        width: 21cm;
        height: 29.7cm;
        /* letter-spacing: +2px; */
    }

    td, th {
        font-family: 'Times New Roman', Times, serif;
        border-top: 1px solid #494949;
        padding: 0 0.5rem;
        font-size: 12px;
    }

    .header-section {
        padding: 1rem;
    }

    .header-section-spacing {
        padding-right: 1rem;
    }

    hr { margin: 0.1rem auto; }


    .debug-border {
        border: 1px solid red
    }

  
</style>

<body>
    <page size="A4">
        <div class="container-fluid header-section">
            <div class="row ">

                <div class="col-2 header-section-spacing justify-content-center align-self-center">
                    <img src="../../../../assets/img/TFES_Logo_Pantone.png" width="100%" />
                </div>

                <div class="col-4 header-section-spacing">
