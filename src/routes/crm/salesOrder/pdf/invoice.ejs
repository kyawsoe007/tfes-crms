                    <p>CO / GST Reg No :2017137520</p>
                </div>

                <div class="col-6 header-section-spacing justify-content-center align-self-center">
                    <img src="../../../../assets/img/tfes-accolades.png" width="32%" />
                    <img src="../../../../assets/img/tfes-accolades.png" width="32%" />
                    <img src="../../../../assets/img/tfes-accolades.png" width="32%" />
                </div>

            </div>


            <h2 class="text-center" style="text-align: center; margin: 0 auto">Commercial Invoice</h1>

            <div class="row">
                <div class="col-8">
                    <h4 style="font-weight: bold">SOLD TO</h4>
                </div>

                <div class="col-4">
                    <h4 style="font-weight: bold">SHIP TO</h4>
                </div>
            </div>
            <hr>

            <div class="row">
                <div class="col-8">
                    <!-- <h4 style="font-weight: bold">PT Apexindo Pratama Duta Tbk.</h4>
                    <h4>Head Office jakarta - Indonesia</h4>
                    <h4>Office 8 Building, 20th - 21st Floor SCBD Lot 28</h4>
                    <h4>JL. Jend. Sudirman Kav. 52-53 Kebayoran Baru. Jakarta 12190</h4> -->
                    <h4> <%= salesOrderPayLoad.paymentAddress %></h4>
                </div>

                <div class="col-4">
                    <!-- <h4 style="font-weight: bold">PT Apexindo Pratama Duta Tbk.</h4>
                    <h4>Kompleks Balikpapan Permai JL. Jendral </h4>
                    <h4>Sudirman Blok L 10-12</h4> -->
                    <h4> <%= salesOrderPayLoad.delAddress %></h4>
                </div>
            </div>
            <hr>

            <div class="row">
                <div class="col-8">
                    <h4 style="font-weight: bold">Customer point of contact: <%= salesOrderPayLoad.buyerName %></h4>
                </div>

                <div class="col-4">
                    <div class="row">
                        <div class="col-6">
                            <h4 style="font-weight: bold">Date:</h4>
                        </div>
                        <div class="col-6">
                            <h4 style="font-weight: bold">19-Feb-21</h4>
                        </div>
                    </div>
                </div>
            </div>
            <hr>

            <div class="row">
                <div class="col-8">
                    <h4>Email: <%= salesOrderPayLoad.buyerEmail %></h4>
                </div>


            </div>
            <hr>

            <div class="row">
                <div class="col-8">
                    <h4 style="font-weight: bold">Customer PO REF: <%= salesOrderPayLoad.custNo %></h4>
                </div>

            </div>
            <hr style="padding-bottom: 0.5rem;">

            <div class="row">
                <div class="col">
                    <h4 style="font-weight: bold">Remarks: <%= salesOrderPayLoad.remarks %></h4>
                </div>
            </div>
            <hr>

        </div>

        <!-- COMMERCIAL INVOICE TABLE SECTION  -->
        <div class="container-fluid">
            <div class="row">
                <div class="col">
                <table width="100%">
                    <tr>
                        <th width="8%">Item</th>
                        <th width="60%">Descriptions </th>
                        <th width="8%">QTY</th>
                        <th width="8%">UOM</th>
                        <th width="8%">Unit Price</th>
                        <th width="8%">Extended Price</th>
                    </tr>

