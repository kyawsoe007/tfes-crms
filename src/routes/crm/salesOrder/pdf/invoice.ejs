                    <tr>
                        <% for(var i = 0; i < salesOrderPayLoad.salesOrderItems.length; i ++) {%>
                            <td> <%= salesOrderPayLoad.salesOrderItems[i].SN %> </td>
                            <td> <%= salesOrderPayLoad.salesOrderItems[i].description %> </td>
                            <td> <%= salesOrderPayLoad.salesOrderItems[i].qty %>  </td>
                            <td> LGT </td>
                            <td> <%= salesOrderPayLoad.salesOrderItems[i].unitPrice %>  </td>
                            <td> <%= salesOrderPayLoad.salesOrderItems[i].extPrice %> </td>
                        <% } %>
                    </tr>
                    <tr>
                        <!-- <th></th>  
                        <th></th>
                        <th style="visibility: hidden">hidden</th>
                        <th></th>
                        <th>Grand Total</th>
                        <th></th> -->

                        <td></td>
                        <td></td>
                        <td colspan="3" style="font-weight: bold; font-size: 16px;">GRAND TOTAL:</td>
                        <td style="font-weight: bold; font-size: 16px;"><%= salesOrderPayLoad.total + salesOrderPayLoad.gst %></td>


                    </tr>
                    <tr>
                        <th style="visibility: hidden">hidden</th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>

                    </tr>
                </table>
                </div>
